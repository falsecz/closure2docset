case FOO: doSomething(); break;
case BAR: doSomethingElse(); break;
default: goog.assert.fail(&amp;#39;Unrecognized type: &amp;#39; + type);
// We have only 2 types - &amp;quot;default:&amp;quot; section is unreachable code.
}
</pre>
