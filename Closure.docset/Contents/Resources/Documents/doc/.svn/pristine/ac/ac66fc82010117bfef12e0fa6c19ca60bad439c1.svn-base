<a name="line1232"></a>  var visited = 0;
<a name="line1233"></a>  while (visited &lt;= numItems) {
<a name="line1234"></a>    var control = this.getChildAt(curIndex);
<a name="line1235"></a>    if (control &amp;&amp; this.canHighlightItem(control)) {
