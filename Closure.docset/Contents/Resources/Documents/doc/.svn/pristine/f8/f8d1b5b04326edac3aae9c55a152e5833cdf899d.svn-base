<a name="line883"></a>
<a name="line884"></a>/**
<a name="line885"></a> * Opens the database.
<a name="line886"></a> */
<a name="line887"></a>goog.gears.Database.prototype.open = function() {
<a name="line888"></a>  if (this.database_ &amp;&amp; this.safeDbId_) {
