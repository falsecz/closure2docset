<a name="line869"></a> *     consumed.
<a name="line870"></a> */
<a name="line871"></a>goog.gears.Database.prototype.getExecutionTime = function() {
<a name="line872"></a>  return this.database_[&#39;executeMsec&#39;] || 0;
<a name="line873"></a>};
<a name="line874"></a>
<a name="line875"></a>
<a name="line876"></a>/**
<a name="line877"></a> * @return {number} The id of the last inserted row.
<a name="line878"></a> */
<a name="line879"></a>goog.gears.Database.prototype.getLastInsertRowId = function() {
