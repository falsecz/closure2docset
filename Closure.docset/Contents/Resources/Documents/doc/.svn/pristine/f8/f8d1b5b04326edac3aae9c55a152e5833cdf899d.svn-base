<a name="line315"></a> */
<a name="line316"></a>goog.gears.Database.prototype.execute = function(sql, var_args) {
<a name="line317"></a>  this.logger_.finer(&#39;Executing SQL: &#39; + sql);
<a name="line318"></a>
<a name="line319"></a>  // TODO(user): Remove when Gears adds more rubust type handling.
<a name="line320"></a>  // Safety measure since Gears behaves very badly if it gets an unexpected
<a name="line321"></a>  // data type.
<a name="line322"></a>  sql = String(sql);
<a name="line323"></a>
<a name="line324"></a>  var args;
<a name="line325"></a>  try {
<a name="line326"></a>    if (arguments.length == 1) {
<a name="line327"></a>      return this.database_.execute(sql);
<a name="line328"></a>    }
<a name="line329"></a>
<a name="line330"></a>    if (arguments.length == 2 &amp;&amp; goog.isArray(arguments[1])) {
<a name="line331"></a>      args = arguments[1];
<a name="line332"></a>    } else {
<a name="line333"></a>      args = goog.array.slice(arguments, 1);
<a name="line334"></a>    }
<a name="line335"></a>    this.logger_.finest(&#39;SQL arguments: &#39; + args);
<a name="line336"></a>
<a name="line337"></a>    // TODO(user): Type safety checking for args?
<a name="line338"></a>    return this.database_.execute(sql, args);
<a name="line339"></a>  } catch (e) {
<a name="line340"></a>    if (args) {
<a name="line341"></a>      sql += &#39;: &#39; + goog.json.serialize(args);
<a name="line342"></a>    }
<a name="line343"></a>
<a name="line344"></a>    throw goog.debug.enhanceError(e, sql);
<a name="line345"></a>  }
<a name="line346"></a>};
