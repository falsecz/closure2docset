<a name="line57"></a>
<a name="line58"></a>
<a name="line59"></a>/**
<a name="line60"></a> * @return {string} The URL for this emoji.
<a name="line61"></a> */
<a name="line62"></a>goog.ui.emoji.Emoji.prototype.getUrl = function() {
<a name="line63"></a>  return this.url_;
<a name="line64"></a>};
<a name="line65"></a>
<a name="line66"></a>
